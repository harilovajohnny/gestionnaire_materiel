{% extends 'base.html.twig' %}

{% block title %}Materiel{% endblock %}

{% block body %}
    <h1>Materiel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ materiel.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ materiel.Name }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ materiel.category }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ materiel.number }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ materiel.description }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ materiel.createdAt ? materiel.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ materiel.updatedAt ? materiel.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_materiels_index') }}">back to list</a>

    <a href="{{ path('app_materiels_edit', {'id': materiel.id}) }}">edit</a>

    {{ include('materiels/_delete_form.html.twig') }}
{% endblock %}
